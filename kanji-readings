#!/usr/bin/env perl
use 5.14.0;
use warnings;
use utf8::all;
use Anki::Database;
use List::MoreUtils 'uniq';
use Lingua::JA::Moji 'kana2katakana', 'kata2hira';

sub kana2hiragana { kata2hira(kana2katakana(shift)) }
my %devoice_table = qw/
    が か  ぎ き  ぐ く  げ け  ご こ
    ざ さ  じ し  ず す  ぜ せ  ぞ そ
    だ た  ぢ ち  づ つ  で て  ど と
    ば は  び ひ  ぶ ふ  べ へ  ぼ ほ
    ぱ は  ぴ ひ  ぷ ふ  ぺ へ  ぽ ほ
/;
my %promote_chiisai = qw/っ つ/;
sub devoice         { join '', map { $devoice_table{$_}   || $_ } split '', shift }
sub promote_chiisai { join '', map { $promote_chiisai{$_} || $_ } split '', shift }

my $db = Anki::Database->new(file => $ENV{ANKI_DECK});

my $ankidir = "$ENV{HOME}/Documents/Anki";
my $core_deck = "$ankidir/core2kcore6k.anki";
my $core_db = Anki::Database->new(file => $core_deck);

my $sth = $db->prepare("
    select kanji.value
        from fields as kanji
            join fieldModels on (kanji.fieldModelId = fieldModels.id)
            join facts on (kanji.factId = facts.id)
        where
            fieldModels.name = '漢字'
            order by facts.created
;");
$sth->execute;

my @kanji;
while (my ($kanji) = $sth->fetchrow_array) {
    push @kanji, $kanji;
}

my $re = do {
    my $re = '[' . join('', @kanji) . ']';
    qr/$re/;
};

my %joyo_readings;
while (<DATA>) {
    my ($kanji, @readings) = map { kana2hiragana($_) } split ' ';
    $joyo_readings{$kanji} = \@readings;
}

$sth = $db->prepare("
    select yomi.value
        from fields as yomi
            join fieldModels on (yomi.fieldModelId = fieldModels.id)
            join models on (fieldModels.modelId = models.id)
            join cards on (yomi.factId = cards.factId)
        where
            models.name = '文'
            and fieldModels.name = '読み'
            and yomi.value <> ''
            and cards.type >= 0
;");
$sth->execute;

my %seen_line;
my %seen_joyo;
my %extra_words;

sub joyo_for {
    my ($kanji, $word, $reading) = @_;

    my @joyo;
    for my $joyo (@{ $joyo_readings{$kanji} }) {
        (my $joyo_re = $joyo) =~ s/-//;

        my $matched = $reading =~ /$joyo_re/;

        if ($matched) {
            push @joyo, $joyo;
        }
    }
    return @joyo == 0 ? undef : \@joyo;
}

while (my ($yomi) = $sth->fetchrow_array) {
    for my $line (split /<br.*?>/, $yomi) {
        next if $seen_line{$line}++;
        unless ($line =~ /^([^【]+)【([^】]+)】$/) {
            warn "invalid reading line: $line";
            next;
        }
        my ($word, $reading) = ($1, $2);
        for my $kanji ($word =~ /\p{Han}/g) {
            my $joyo = joyo_for($kanji, $word, $reading)
                    || joyo_for($kanji, $word, devoice($reading))
                    || joyo_for($kanji, $word, promote_chiisai($reading))
                    || joyo_for($kanji, $word, devoice(promote_chiisai($reading)));

            if ($joyo) {
                for (@$joyo) { $seen_joyo{$kanji}{$_}++ }
            }
            else {
                push @{ $extra_words{$kanji} }, $line;
            }
        }
    }
}

for my $kanji (@kanji) {
    print "$kanji: ";
    while (my $joyo = shift @{ $joyo_readings{$kanji} }) {
        if ($seen_joyo{$kanji}{$joyo}) {
            print "\e[1;32m";
        }
        else {
            print "\e[1;31m";
        }
        print $joyo;
        print "\e[m";
        print "、" if @{ $joyo_readings{$kanji} };
    }
    for (@{ $extra_words{$kanji} }) {
        print "、$_";
    }
    print "\n";
}

__END__
